      <div class="container-fluid py-4">
        <div class="row">
          <div class="col-12">
            <div class="card mb-4">
              <div class="card-header pb-0">
                <h6>Ticket</h6>
              </div>
              <div class="card-body px-0 mt-2 pt-0 pb-2">
                <div class="table-responsive p-0">
                  <table class="table align-items-center mb-0">
                    <thead>
                      <tr>
                        <th
                          class="text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Ticket Raiser Name
                        </th>
                        <th
                          class="text-uppercase text-secondary text-xs font-weight-bolder opacity-7 ps-2"
                        >
                          Category
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Status
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Priority
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Created
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Worked Since
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Updated At
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Closed At
                        </th>

                        <th class="text-secondary opacity-7"></th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td>
                          <div class="d-flex px-2 py-1">
                            <div
                              class="d-flex flex-column justify-content-center"
                            >
                              <h6 class="mb-0 text-sm"><%= ticket.name %></h6>
                              <p class="text-xs text-secondary mb-0">
                                <%= ticket.email %>
                              </p>
                            </div>
                          </div>
                        </td>
                        <td>
                          <p class="text-xs font-weight-bold mb-0">
                            <%= ticket.category %>
                          </p>
                        </td>
                        <td class="align-middle text-center text-sm">
                          <% if (ticket.status === 'open') { %>
                          <span class="badge badge-sm bg-gradient-info"
                            ><%= ticket.status %></span
                          >
                          <% } else if (ticket.status === 'in progress') { %>
                          <span class="badge badge-sm bg-gradient-success"
                            ><%= ticket.status %></span
                          >
                          <% } else { %>
                          <span class="badge badge-sm bg-gradient-secondary"
                            ><%= ticket.status %></span
                          >
                          <% } %>
                        </td>
                        <td class="align-middle text-center">
                          <% if (ticket.priority === 'high') { %>
                          <span class="badge badge-sm bg-gradient-danger"
                            ><%= ticket.priority %></span
                          >
                          <% } else if (ticket.priority === 'medium') { %>
                          <span class="badge badge-sm bg-gradient-warning"
                            ><%= ticket.priority %></span
                          >
                          <% } else { %>
                          <span class="badge badge-sm bg-gradient-light"
                            ><%= ticket.priority %></span
                          >
                          <% } %>
                        </td>
                        <td class="align-middle text-center">
                          <% if (ticket.createdAt) { %>
                          <span class="text-secondary text-xs font-weight-bold"
                            ><% const createdAt = new Date(ticket.createdAt); %>
                            <%= createdAt.toLocaleString() %></span
                          >
                          <% } else { %> N/A <% } %>
                        </td>
                        <td class="align-middle text-center">
                          <% if (ticket.workedSince) { %>
                          <span class="text-secondary text-xs font-weight-bold"
                            ><% const workedSince = new
                            Date(ticket.workedSince); %> <%=
                            workedSince.toLocaleString() %></span
                          >
                          <% } else { %> N/A <% } %>
                        </td>
                        <td class="align-middle text-center">
                          <% if (ticket.updatedAt) { %>
                          <span class="text-secondary text-xs font-weight-bold"
                            ><% const updatedAt = new Date(ticket.updatedAt); %>
                            <%= updatedAt.toLocaleString() %></span
                          >
                          <% } else { %> N/A <% } %>
                        </td>
                        <td class="align-middle text-center">
                          <% if (ticket.closedAt) { %>
                          <span class="text-secondary text-xs font-weight-bold"
                            ><% const closedAt = new Date(ticket.closedAt); %>
                            <%= closedAt.toLocaleString() %></span
                          >
                          <% } else { %> N/A <% } %>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                  <table class="table align-items-center mb-0">
                    <thead>
                      <tr>
                        <th
                          class="text-uppercase text-xs font-weight-bolder opacity-7"
                        >
                          <h6>Ticket Issue</h6>
                        </th>
                      </tr>
                    </thead>
                    <hr class="p-2" />
                    <tbody>
                      <tr class="mt-5">
                        <td style="white-space: inherit" class="p-4">
                          <!-- Your text goes here -->
                          <%= ticket.message %>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-12">
            <div class="card mb-4">
              <div class="card-header pb-0">
                <h6>Assigned Employee</h6>
              </div>
              <div class="card-body px-0 mt-2 pt-0 pb-2">
                <div class="table-responsive p-0">
                  <table class="table align-items-center mb-0">
                    <% if (employeeData) { %>
                    <thead>
                      <tr>
                        <th
                          class="text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Employee Name
                        </th>
                        <th
                          class="text-uppercase text-secondary text-xs font-weight-bolder opacity-7 ps-2"
                        >
                          Employee Email
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Department
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Employee Id
                        </th>
                        <th
                          class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-7"
                        >
                          Actions
                        </th>
                        <th class="text-secondary opacity-7"></th>
                      </tr>
                    </thead>
                   
                    <tbody>
                      <% if (employeeData.employeeName) { %>
                      <tr>
                        <td class="text-center justify-content align-items">
                          <p class="text-sm font-weight-bold mb-0 mr-5">
                            <%= employeeData.employeeName %>
                          </p>
                        </td>
                        <td class="align-middle text-center">
                          <p class="text-sm font-weight-bold mb-0">
                            <%= employeeData.employeeEmail %>
                          </p>
                        </td>
                        <td class="align-middle text-center">
                          <p class="text-sm font-weight-bold mb-0">
                            <%= employeeData.employeeDepartment %>
                          </p>
                        </td>
                        <td class="align-middle text-center">
                          <p class="text-sm font-weight-bold mb-0">
                            <%= employeeData.employeeId %>
                          </p>
                        </td>
                        <td class="align-middle text-center">
                          <a  href="/admin/admin-dashboard/employee-detail/<%= employeeData.employeeSecretId %>" class="btn btn-info">View Detail</a>
                        </td>
                      </tr>
                      <% } else { %>
                      <tr>
                        <td class="text-center justify-content align-items">
                          <p class="text-sm font-weight-bold mb-0 mr-5">N/A</p>
                        </td>
                        <td class="align-middle text-center">
                          <p class="text-sm font-weight-bold mb-0">N/A</p>
                        </td>
                        <td class="align-middle text-center">
                          <p class="text-sm font-weight-bold mb-0">N/A</p>
                        </td>
                        <td class="align-middle text-center">
                          <p class="text-sm font-weight-bold mb-0">N/A</p>
                        </td>
                        <td class="align-middle text-center">
                          <p class="text-sm font-weight-bold mb-0">N/A</p>
                        </td>
                      </tr>
                      <% } %>
                    </tbody>
                    <% } else { %>
                      <h3 class="p-3">No Employee Assigned</h3>
                    <% } %>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </main>

    <!-- Control Center for Soft Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="/js/soft-ui-dashboard.min.js"></script>
    <script src="/js/core/bootstrap.bundle.min.js"></script>
  </body>
</html>
